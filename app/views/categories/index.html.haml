.bar-title
  %h2.pull-left= t('model.category')
  .btn-group.pull-right
    = link_to new_category_path, class: 'btn btn-default', title: t('category.new') do
      %span.fa.fa-plus

%form.well.form-inline.search
  .row
    = search_input('category')

#categories.partial
  = render :partial => 'listing', :locals => {:categories => @objects}

